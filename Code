      "source": [
        "#Adding the cluster label to each venue in the hotels data frame\n",
        "X = X[['id','cluster_label']]\n",
        "clustered_hotels = hotels_df_filtered.merge(X, left_on='id',right_on='id')\n",
        "clustered_hotels.head()"
      ],
      "execution_count": 230,
      "outputs": [
        {
