      "source": [
        "#creating a map of bars\n",
        "restaurants_map = folium.Map(location=[latitude, longitude], zoom_start=15) # generate map centred around Mexico City\n",
        "\n",
        "# add the restaurants as purple circle markers\n",
        "for lat, lng, label in zip(restaurants_df_cleaned.lat, restaurants_df_cleaned.lng, restaurants_df_cleaned.categories):\n",
        "    folium.CircleMarker(\n",
        "        [lat, lng],\n",
        "        radius=5,\n",
        "        color='purple',\n",
        "        popup=label,\n",
        "        fill = True,\n",
        "        fill_color='purple',\n",
        "        fill_opacity=0.6\n",
        "    ).add_to(restaurants_map)\n",
        "\n",
        "# display map\n",
        "restaurants_map"
      ],
      "execution_count": 239,
      "outputs": [
        {
